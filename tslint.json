{
  "extends": [
    "tslint:latest",
    "tslint-react",
    "tslint-react-hooks"
  ],
  "rules": {
    "interface-name": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "jsx-no-multiline-js": false,
    "quotemark": false,
    "arrow-parens": false,
    "react-hooks-nesting": "error",
    "no-submodule-imports": false,
    "no-implicit-dependencies": false
  }
}
